<h1>all events</h1>

<% @events.each do |event| %>
<br>
    <div>
        <p>Title: <%= event.title %></p>
        <p>Description: <%= event.description %></p>
        <p>Date: <%= event.date %></p>
        <p>Time: <%= event.time %></p>
        <p>Creator: <%= event.creator.username %></p>
    </div>
<% end %>
