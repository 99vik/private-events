<div class='navbar'>
      <% if user_signed_in? %>
        <div>
            <%= link_to "Home", root_path %>
            <%= link_to "Create new event", new_event_path %>
        </div>
        <div>
            <div>Logged in as <%= current_user.username %></div>
            <%= link_to "Profile", controller: "/users", action: "show", id: current_user.id %>
            <%= link_to("Log out", destroy_user_session_path, method: :delete, data: { turbo_method: :delete }) %>
        </div>
      <% else %>
            <div><%= link_to "Home", root_path %></div>
            <div>
              <%= link_to "Log In", new_user_session_path %>
              <%= link_to "Sign Up", new_user_registration_path %>
            </div>
      <% end %>
</div>